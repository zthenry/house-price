<?xml version="1.0" encoding="UTF-8" ?>     
<!DOCTYPE configuration     
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"     
    "http://mybatis.org/dtd/mybatis-3-config.dtd">  
<configuration>  
    <typeAliases>  
        <!--给实体类起一个别名  -->  
        <typeAlias type="com.henry.house.price.model.HousePrice" alias="HousePrice" />  
    </typeAliases>  
    <mappers>  
        <!--userMapper.xml装载进来  同等于把“dao”的实现装载进来 -->  
        <mapper resource="com/henry/house/price/dao/impl/HousePriceDao.xml" />  
    </mappers>  
      
</configuration>  